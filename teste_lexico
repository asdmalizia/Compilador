function fun1 ( id : string ) : integer {
	var c : boolean;
	var a : integer;
	var b : integer;
	var i : integer;
	c = true && false;

	while( a != b)
		i = i+1;
}

function primo(n: integer): boolean {
	var divisor: integer;
	divisor = 2;
	do {
		if(mmc(n,divisor) == n){
			return true;
		} else {
			divisor = divisor + 'a';
		}
	} while(divisor < sqrt(n));
	return false;
}